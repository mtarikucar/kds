{
  "productName": "KDS POS",
  "version": "1.0.0",
  "identifier": "com.kds.pos",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:5174",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "KDS POS",
        "width": 1280,
        "height": 800,
        "minWidth": 1024,
        "minHeight": 768,
        "center": true,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/icon.png"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "Business",
    "shortDescription": "Restaurant POS System",
    "longDescription": "Professional Restaurant POS and Kitchen Display System",
    "copyright": "Copyright (c) 2024. All rights reserved.",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {}
}
